journal = {Medical Care},
keywords = {47,and population sciences,cost analysis,cost and,division of cancer control,from the,health care costs,health services research,med care 2009,medicare,national,neoplasms,s56,s63,seer program},
number = {Supplement},
pages = {S56--S63},
pmid = {19536010},
title = {{Comparison of Approaches for Estimating Incidence Costs of Care for Colorectal Cancer Patients}},
url = {http://insights.ovid.com/crossref?an=00005650-200907001-00010},
volume = {47},
year = {2009}
}
